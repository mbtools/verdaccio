{
  "name": "@verdaccio-pro/storage-sql",
  "version": "1.0.0",
  "description": "SQL Storage Plugin for Verdaccio Pro (Postgres)",
  "keywords": [
    "verdaccio",
    "verdaccio-pro",
    "plugin",
    "sql",
    "database",
    "postgres"
  ],
  "author": {
    "name": "Marc Bernard",
    "url": "https://github.com/mbtools"
  },
  "license": "FSL-1.1-MIT",
  "homepage": "https://verdaccio.pro",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/verdaccio-pro/verdaccio-pro.git",
    "directory": "packages/storage-sql"
  },
  "bugs": {
    "url": "https://github.com/verdaccio-pro/verdaccio-pro/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/verdaccio-pro"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@verdaccio/config": "8.0.0-next-8.21",
    "@verdaccio/core": "8.0.0-next-8.21",
    "@verdaccio/logger": "8.0.0-next-8.21",
    "debug": "4.4.1",
    "dotenv": "17.2.0",
    "dotenv-expand": "12.0.2",
    "drizzle-orm": "0.44.3",
    "pg": "8.16.3",
    "postgres": "3.4.7",
    "zod": "4.0.8"
  },
  "devDependencies": {
    "@eslint/js": "9.31.0",
    "@stylistic/eslint-plugin": "5.2.2",
    "@types/debug": "4.1.12",
    "@types/node": "24.1.0",
    "@types/pg": "8.15.4",
    "@verdaccio/types": "13.0.0-next-8.7",
    "cross-env": "7.0.3",
    "drizzle-kit": "0.31.4",
    "eslint": "9.31.0",
    "tsup": "8.5.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.38.0",
    "vitest": "3.2.4"
  },
  "scripts": {
    "build": "tsup",
    "test": "cross-env NODE_ENV=test vitest run --max-concurrency=1",
    "lint": "eslint src",
    "db:studio": "npx drizzle-kit studio",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "cross-env DB_MIGRATING=true tsx src/db/tools/migrate.ts",
    "db:seed": "cross-env DB_SEEDING=true tsx src/db/tools/seed.ts",
    "db:reset": "cross-env DB_RESET=true tsx src/db/tools/reset.ts"
  }
}
